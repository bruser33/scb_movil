<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cargo.usuario.cargoex">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />
    <uses-feature android:name="android.hardware.location.gps" />
    <application
        android:requestLegacyExternalStorage="true"
        android:allowBackup="true"
        android:icon="@mipmap/logoapp"
        android:configChanges="orientation"
        android:screenOrientation="portrait"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.example.usuario.cargoex.MainActivity"/>
        <activity  android:name="com.example.usuario.cargoex.Login"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.usuario.cargoex.Dashboard" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:windowSoftInputMode="adjustPan"
            android:name="com.example.usuario.cargoex.IngresoOd" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Confirmacion" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Confimacion2" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Devoluciones" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.EntregaNormal" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Biometria" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.EntregaBiometrica" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.ConfimacionRetiro" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.DatosRetiroHuella" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.DatosRetiroNormal" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Retiro" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Masivos" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Equipo" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.PdfViewer" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.Agente" android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name="com.example.usuario.cargoex.util.Modal" android:theme="@style/AppTheme.PopMe"/>
        <activity android:name="com.example.usuario.cargoex.util.Modal2" android:theme="@style/AppTheme.PopMe"/>
        <activity android:name="com.example.usuario.cargoex.util.ModalGestiones" android:theme="@style/AppTheme.PopMe"/>
        <activity android:name="com.example.usuario.cargoex.util.ModalVersion" android:theme="@style/AppTheme.PopMe"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>