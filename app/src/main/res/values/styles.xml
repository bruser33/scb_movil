<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="SpinnerAppTheme" parent="android:Widget.Spinner">
        <item name="android:background">@color/White</item>
        <item name="android:dropDownSelector">@color/apptheme_color</item>
    </style>
    <style name="AppTheme.PopMe">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="item">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/titillium_web</item>
        <item name="android:textColor">#202156</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="item2">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/titillium_web</item>
        <item name="android:textColor">#202156</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/modal2</item>
    </style>
    <style name="item3">
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">@font/titillium_web</item>
        <item name="android:textColor">#FF0000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/modal2</item>
    </style>
</resources>
