<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/fondo5">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginStart="2dp"
            android:layout_marginTop="-10dp"
            app:srcCompat="@mipmap/logorojo2" />


        <TextView
            android:id="@+id/fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="7dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="5656865"
            android:textColor="#202156"
            android:textSize="23sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/i"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="-200dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="INGENIERIA DE SOFTWARE"
            android:textColor="#202156"
            android:textSize="25sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lider"
            android:layout_width="350dp"
            android:layout_height="137dp"
            android:layout_gravity="center"
            android:layout_marginTop="-100dp"
            android:background="@drawable/action2"
            android:layout_weight="1"
            android:paddingLeft="100dp"
            android:paddingTop="5dp"
            android:fontFamily="@font/titillium_web"
            android:text="Nombre: Jhonathan Olaguibel Torres. \nCargo: Lider de proyectos informaticos.\nEjecución: Backend AWS (EC2, RDS, LAMBDA, API GATEWAY), Laravel PHP.\njonathan.olaguibel@gmail.com\n+56 940045364"
            android:textColor="#202156"
            android:textSize="13sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/foto1"
            android:layout_width="90dp"
            android:layout_height="180dp"
            android:layout_gravity="center"
            android:layout_marginRight="123dp"
            android:layout_marginTop="-100dp"
            app:srcCompat="@drawable/foto1"/>

        <TextView
            android:id="@+id/dev"
            android:layout_width="350dp"
            android:layout_height="137dp"
            android:layout_gravity="center"
            android:layout_marginTop="50dp"
            android:background="@drawable/action2"
            android:layout_weight="1"
            android:paddingLeft="100dp"
            android:paddingTop="5dp"
            android:fontFamily="@font/titillium_web"
            android:text="Nombre: Pedro Luis Rico Barajas. \nCargo: Desarrollador Semi Senior.\nEjecución: Frontend Android Nativo, React, React Native, Redux. \npedro.luis.rico43@gmail.com\n+56 944781779"
            android:textColor="#202156"
            android:textSize="13sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/foto2"
            android:layout_width="90dp"
            android:layout_height="180dp"
            android:layout_gravity="center"
            android:layout_marginRight="123dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/foto2"/>

        <TextView
            android:id="@+id/nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="50dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="H REQUELME CORDOVA"
            android:textColor="#202156"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="630dp"
            android:layout_weight="1"
            android:fontFamily="@font/titillium_web"
            android:text="@string/version"
            android:textColor="#0E004D"
            android:layout_marginEnd="3dp"
            android:textSize="15sp" />



    </FrameLayout>
</android.support.constraint.ConstraintLayout>